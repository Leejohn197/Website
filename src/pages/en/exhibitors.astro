---
import Layout from '../../layouts/Layout.astro';
import Navbar from '../../components/Navbar.astro';
import Footer from '../../components/common/Footer.astro';
---

<Layout 
  title="Exhibitor Services | Jakarta International Expo 2026"
  description="Book your stand at Jakarta International Expo 2026. Explore exhibitor packages, booth options, and services for Southeast Asia's premier trade show."
  lang="en"
>
  <Navbar />
  
  <main class="pt-20">
    <!-- Featured Events Section - Starts immediately after navbar -->
    <section class="bg-gradient-to-br from-primary-900 to-primary-800 text-white section-padding border-b-4 border-batik">
      <div class="container-custom">
        <h2 class="font-heading text-3xl md:text-4xl font-bold text-center mb-4">
          Featured Events
        </h2>
        <p class="text-center text-primary-100 mb-12 max-w-2xl mx-auto">
          Major exhibition events coming to Jakarta International Convention Center in 2026
        </p>

        <!-- Row 1: First 3 Events -->
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8 mb-8">
          <!-- Event 1: Tech Expo -->
          <div class="bg-white/10 backdrop-blur-sm rounded-xl p-6 hover:bg-white/20 transition-all">
            <div class="relative h-40 mb-4 rounded-lg overflow-hidden">
              <img src="/images/events/tech-expo.jpg" alt="Tech Expo" class="w-full h-full object-cover" />
              <div class="absolute top-2 right-2 bg-secondary-500 text-neutral-900 px-3 py-1 rounded-full text-xs font-bold">
                FEATURED
              </div>
              <div class="absolute bottom-2 left-2 bg-primary-500 text-white px-3 py-1 rounded-md text-xs font-semibold">
                TECHNOLOGY
              </div>
            </div>
            <h3 class="font-heading text-2xl font-bold mb-3">Jakarta Tech Expo 2026</h3>
            <div class="space-y-2 mb-4">
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                </svg>
                <span>March 15-18, 2026</span>
              </div>
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                </svg>
                <span>JIExpo Kemayoran</span>
              </div>
            </div>
            <p class="text-primary-100 text-sm leading-relaxed mb-4">
              Showcase cutting-edge electronics, AI, IoT, and digital innovations. Connect with tech industry leaders, startups, and investors.
            </p>
            <div class="flex flex-wrap gap-2">
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">AI & ML</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">IoT</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Cloud</span>
            </div>
          </div>

          <!-- Event 2: Food & Beverage -->
          <div class="bg-white/10 backdrop-blur-sm rounded-xl p-6 hover:bg-white/20 transition-all">
            <div class="relative h-40 mb-4 rounded-lg overflow-hidden">
              <img src="/images/events/food-beverage.jpg" alt="Food & Beverage Fair" class="w-full h-full object-cover" />
              <div class="absolute top-2 right-2 bg-secondary-500 text-neutral-900 px-3 py-1 rounded-full text-xs font-bold">
                FEATURED
              </div>
              <div class="absolute bottom-2 left-2 bg-accent-500 text-white px-3 py-1 rounded-md text-xs font-semibold">
                F&B
              </div>
            </div>
            <h3 class="font-heading text-2xl font-bold mb-3">Indonesia Food & Beverage Fair</h3>
            <div class="space-y-2 mb-4">
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                </svg>
                <span>April 5-8, 2026</span>
              </div>
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                </svg>
                <span>Jakarta Convention Center</span>
              </div>
            </div>
            <p class="text-primary-100 text-sm leading-relaxed mb-4">
              Explore Indonesia's rich culinary heritage and international food trends. From traditional delicacies to innovative beverages.
            </p>
            <div class="flex flex-wrap gap-2">
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Indonesian Cuisine</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">International</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Beverages</span>
            </div>
          </div>

          <!-- Event 3: Manufacturing -->
          <div class="bg-white/10 backdrop-blur-sm rounded-xl p-6 hover:bg-white/20 transition-all">
            <div class="relative h-40 mb-4 rounded-lg overflow-hidden">
              <img src="/images/events/manufacturing.jpg" alt="Manufacturing Summit" class="w-full h-full object-cover" />
              <div class="absolute bottom-2 left-2 bg-primary-500 text-white px-3 py-1 rounded-md text-xs font-semibold">
                MANUFACTURING
              </div>
            </div>
            <h3 class="font-heading text-2xl font-bold mb-3">Southeast Asia Manufacturing Summit</h3>
            <div class="space-y-2 mb-4">
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                </svg>
                <span>May 12-15, 2026</span>
              </div>
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                </svg>
                <span>JIExpo Kemayoran</span>
              </div>
            </div>
            <p class="text-primary-100 text-sm leading-relaxed mb-4">
              Explore Industry 4.0, automation, and smart manufacturing solutions. Showcase heavy machinery, production technology, and supply chain innovations.
            </p>
            <div class="flex flex-wrap gap-2">
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Automation</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Industry 4.0</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Machinery</span>
            </div>
          </div>
        </div>

        <!-- Row 2: Next 3 Events -->
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
          <!-- Event 4: Textile & Fashion -->
          <div class="bg-white/10 backdrop-blur-sm rounded-xl p-6 hover:bg-white/20 transition-all">
            <div class="relative h-40 mb-4 rounded-lg overflow-hidden">
              <img src="/images/events/textile-fashion.jpg" alt="Textile & Fashion Expo" class="w-full h-full object-cover" />
              <div class="absolute bottom-2 left-2 bg-accent-500 text-white px-3 py-1 rounded-md text-xs font-semibold">
                FASHION
              </div>
            </div>
            <h3 class="font-heading text-2xl font-bold mb-3">Indonesia Textile & Fashion Expo</h3>
            <div class="space-y-2 mb-4">
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                </svg>
                <span>June 10-13, 2026</span>
              </div>
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                </svg>
                <span>JIExpo Kemayoran</span>
              </div>
            </div>
            <p class="text-primary-100 text-sm leading-relaxed mb-4">
              Showcase Batik, traditional textiles, and modern fashion designs. Connect with fashion designers, textile manufacturers, and retailers.
            </p>
            <div class="flex flex-wrap gap-2">
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Batik</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Fashion Design</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Textiles</span>
            </div>
          </div>

          <!-- Event 5: Automotive -->
          <div class="bg-white/10 backdrop-blur-sm rounded-xl p-6 hover:bg-white/20 transition-all">
            <div class="relative h-40 mb-4 rounded-lg overflow-hidden">
              <img src="/images/events/automotive.jpg" alt="Automotive Expo" class="w-full h-full object-cover" />
              <div class="absolute bottom-2 left-2 bg-primary-500 text-white px-3 py-1 rounded-md text-xs font-semibold">
                AUTOMOTIVE
              </div>
            </div>
            <h3 class="font-heading text-2xl font-bold mb-3">Jakarta International Auto Show</h3>
            <div class="space-y-2 mb-4">
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                </svg>
                <span>July 20-23, 2026</span>
              </div>
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                </svg>
                <span>Jakarta Convention Center</span>
              </div>
            </div>
            <p class="text-primary-100 text-sm leading-relaxed mb-4">
              Display latest car models, electric vehicles, motorcycles, and automotive parts. Connect manufacturers, dealers, and car enthusiasts.
            </p>
            <div class="flex flex-wrap gap-2">
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Electric Vehicles</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Green Energy</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Auto Parts</span>
            </div>
          </div>

          <!-- Event 6: Healthcare & Medical -->
          <div class="bg-white/10 backdrop-blur-sm rounded-xl p-6 hover:bg-white/20 transition-all">
            <div class="relative h-40 mb-4 rounded-lg overflow-hidden">
              <img src="/images/events/healthcare.jpg" alt="Healthcare Expo" class="w-full h-full object-cover" />
              <div class="absolute bottom-2 left-2 bg-accent-500 text-white px-3 py-1 rounded-md text-xs font-semibold">
                HEALTHCARE
              </div>
            </div>
            <h3 class="font-heading text-2xl font-bold mb-3">Southeast Asia Healthcare & Medical Expo</h3>
            <div class="space-y-2 mb-4">
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                </svg>
                <span>August 15-18, 2026</span>
              </div>
              <div class="flex items-center gap-2 text-primary-100">
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                </svg>
                <span>JIExpo Kemayoran</span>
              </div>
            </div>
            <p class="text-primary-100 text-sm leading-relaxed mb-4">
              Showcase medical equipment, pharmaceutical products, and health tech innovations. Meet healthcare professionals and hospital administrators.
            </p>
            <div class="flex flex-wrap gap-2">
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Medical Devices</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Pharma</span>
              <span class="bg-white/20 px-3 py-1 rounded-full text-xs">Health Tech</span>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Benefits Section -->
    <section class="bg-white section-padding">
      <div class="container-custom">
        <h2 class="font-heading text-3xl md:text-4xl font-bold text-primary-900 text-center mb-12">
          Why Exhibit With Us?
        </h2>
        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
          
          <div class="bg-neutral-50 rounded-xl p-8 border-l-4 border-primary-500">
            <div class="w-14 h-14 bg-primary-100 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-8 h-8 text-primary-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"></path>
              </svg>
            </div>
            <h3 class="font-heading text-xl font-bold text-primary-900 mb-3">Targeted Audience</h3>
            <p class="text-neutral-600">Connect with 50,000+ qualified buyers, distributors, and decision-makers actively seeking new partnerships.</p>
          </div>

          <div class="bg-neutral-50 rounded-xl p-8 border-l-4 border-secondary-500">
            <div class="w-14 h-14 bg-secondary-100 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-8 h-8 text-secondary-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3.055 11H5a2 2 0 012 2v1a2 2 0 002 2 2 2 0 012 2v2.945M8 3.935V5.5A2.5 2.5 0 0010.5 8h.5a2 2 0 012 2 2 2 0 104 0 2 2 0 012-2h1.064M15 20.488V18a2 2 0 012-2h3.064M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
              </svg>
            </div>
            <h3 class="font-heading text-xl font-bold text-primary-900 mb-3">Regional Reach</h3>
            <p class="text-neutral-600">Access markets across Indonesia, ASEAN, and beyond with participants from 30+ countries.</p>
          </div>

          <div class="bg-neutral-50 rounded-xl p-8 border-l-4 border-accent-500">
            <div class="w-14 h-14 bg-accent-100 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-8 h-8 text-accent-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
              </svg>
            </div>
            <h3 class="font-heading text-xl font-bold text-primary-900 mb-3">Premium Location</h3>
            <p class="text-neutral-600">Located in Jakarta's SCBD district, easily accessible with world-class facilities.</p>
          </div>

        </div>
      </div>
    </section>

    <!-- Booth Packages -->
    <section class="bg-batik-subtle bg-secondary-50 section-padding">
      <div class="container-custom">
        <h2 class="font-heading text-3xl md:text-4xl font-bold text-primary-900 text-center mb-4">
          Booth Packages
        </h2>
        <p class="text-center text-neutral-600 mb-12 max-w-2xl mx-auto">
          Choose the perfect package for your exhibition needs
        </p>

        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
          
          <!-- Standard Package -->
          <div class="bg-white rounded-2xl p-8 shadow-card hover:shadow-card-hover transition-all">
            <h3 class="font-heading text-2xl font-bold text-primary-900 mb-2">Standard</h3>
            <div class="text-4xl font-bold text-primary-600 mb-6">9m²</div>
            <ul class="space-y-3 mb-8">
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-green-500 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-neutral-600">3m x 3m booth space</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-green-500 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-neutral-600">Basic furniture set</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-green-500 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-neutral-600">Company name board</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-green-500 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-neutral-600">2 exhibitor badges</span>
              </li>
            </ul>
            <a href="/en/contact" class="btn btn-primary w-full text-center">Inquire Now</a>
          </div>

          <!-- Premium Package -->
          <div class="bg-gradient-to-br from-primary-600 to-primary-800 rounded-2xl p-8 shadow-card-hover transform scale-105 relative">
            <div class="absolute -top-4 left-1/2 -translate-x-1/2 bg-secondary-500 text-neutral-900 px-4 py-1 rounded-full text-sm font-bold">
              POPULAR
            </div>
            <h3 class="font-heading text-2xl font-bold text-white mb-2">Premium</h3>
            <div class="text-4xl font-bold text-secondary-400 mb-6">18m²</div>
            <ul class="space-y-3 mb-8">
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-secondary-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-primary-100">6m x 3m booth space</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-secondary-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-primary-100">Premium furniture set</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-secondary-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-primary-100">Custom booth design</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-secondary-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-primary-100">5 exhibitor badges</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-secondary-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                </svg>
                <span class="text-primary-100">Featured listing</span>
              </li>
            </ul>
            <a href="/en/contact" class="btn bg-secondary-500 hover:bg-secondary-600 text-neutral-900 w-full text-center font-bold">Inquire Now</a>
          </div>

          <!-- VIP Package -->
          <div class="bg-white rounded-2xl p-8 shadow-card hover:shadow-card-hover transition-all">
            <h3 class="font-heading text-2xl font-bold text-primary-900 mb-2">VIP</h3>
